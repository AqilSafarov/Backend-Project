@model VmProcess
@{
    Layout = null;
}
<section id="our-process" class="padding bgdark">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 text-center">
                <div class="heading-title whitecolor wow fadeInUp" data-wow-delay="300ms">
                    <span>@Model.Process.Subtitle</span>
                    <h2 class="font-normal">@Model.Process.Title</h2>
                </div>
            </div>
        </div>
        <div class="row">
            <ul class="process-wrapp">
                @foreach (var item in Model.ProcessType)
                {
                    <li class="whitecolor wow fadeIn" data-wow-delay="600ms">
                        <span class="pro-step bottom20">@item.Number</span>
                        <p class="fontbold bottom25">@item.Title</p>
                        <p class="mt-n2 mt-sm-0">@item.Desc</p>
                    </li>

                }
            </ul>
        </div>
    </div>
</section>
